{
  "name": "robole/sulu-form-captcha-bundle",
  "description": "Extension for SuluFormBundle that adds third-party captcha providers",
  "type": "symfony-plugin",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Robole\\SuluFormCaptchaBundle\\": ""
    }
  },
  "authors": [
    {
      "name": "FlxRobole",
      "email": "felix.saenger@robole.de"
    }
  ],
  "homepage": "https://github.com/robole-dev/sulu-form-captcha-bundle",
  "require": {
    "php": "^8.2",
    "jackalope/jackalope-doctrine-dbal": "*",
    "sulu/form-bundle": "^2.5",
    "symfony/config": "^6.0 | ^7.0",
    "symfony/dependency-injection": "^6.0 | ^7.0",
    "symfony/framework-bundle": "^6.0 | ^7.0",
    "symfony/translation": "^6.0 || ^7.0",
    "twig/twig": "^1.41 || ^2.6 || ^3.0"
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true,
      "php-http/discovery": true
    },
    "sort-packages": true
  },
  "require-dev": {
    "cors/friendly-captcha-bundle": "^1.0",
    "friendsofphp/php-cs-fixer": "^3.68",
    "gregwar/captcha-bundle": "^2.3",
    "pixelopen/cloudflare-turnstile-bundle": "^0.4.1",
    "tito10047/altcha-bundle": "^2.0"
  },
  "scripts": {
    "php-cs": "@php vendor/bin/php-cs-fixer fix --verbose --diff --dry-run",
    "php-cs-fix": "@php vendor/bin/php-cs-fixer fix"
  }
}
